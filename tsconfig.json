{
  "include": [
    "src"
  ],
  "compilerOptions": {
    "typeRoots": [
      "types"
    ],
    "rootDir": "src",
    "strict": true,
    "allowJs": true,
    "outDir": "dist/Axiom/",
    "target": "ES5",
    "module": "AMD",
    "lib": [
      "ES2015",
      "DOM"
    ],
    "moduleResolution": "Node",
    "resolveJsonModule": true
  }
}
